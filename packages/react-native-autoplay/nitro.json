{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": ["swe", "iternio", "reactnativeautoplay"],
  "ios": {
    "iosModuleName": "ReactNativeAutoPlay"
  },
  "android": {
    "androidNamespace": ["swe", "iternio", "reactnativeautoplay"],
    "androidCxxLibName": "ReactNativeAutoPlay"
  },
  "autolinking": {
    "AutoPlay": {
      "kotlin": "HybridAutoPlay",
      "swift": "HybridAutoPlay"
    },
    "AndroidAutoTelemetry": {
      "kotlin": "HybridAndroidAutoTelemetry"
    },
    "ListTemplate": {
      "kotlin": "HybridListTemplate",
      "swift": "HybridListTemplate"
    },
    "InformationTemplate": {
      "kotlin": "HybridInformationTemplate",
      "swift": "HybridInformationTemplate"
    },
    "GridTemplate": {
      "kotlin": "HybridGridTemplate",
      "swift": "HybridGridTemplate"
    },
    "MapTemplate": {
      "kotlin": "HybridMapTemplate",
      "swift": "HybridMapTemplate"
    },
    "MessageTemplate": {
      "kotlin": "HybridMessageTemplate",
      "swift": "HybridMessageTemplate"
    },
    "SearchTemplate": {
      "kotlin": "HybridSearchTemplate",
      "swift": "HybridSearchTemplate"
    },
    "CarPlayDashboard": {
      "swift": "HybridCarPlayDashboard"
    },
    "Cluster": {
      "kotlin": "HybridCluster",
      "swift": "HybridCluster"
    }
  },
  "ignorePaths": ["**/node_modules"]
}
